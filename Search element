//Search an element in array

arr =[45,7,34,57,22]

search_element = int(input("Enter the Element to be searched :"))

Found = False

for i in range(len(arr)):
    if arr[i]== search_element:
        Found = True
        print(f"Element {search_element} found in the array {i}")
        
if not Found:
    print (f"Element {search_element} not found in the array")

